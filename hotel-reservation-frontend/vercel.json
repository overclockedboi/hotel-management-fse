{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/hotel-reservation/browser"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)\\.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/dist/hotel-reservation/browser/$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}