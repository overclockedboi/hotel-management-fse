{
    "version": 2,
    "builds": [
        {
            "src": "angular.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/hotel-reservation/browser"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)\\.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/$1.js"
        },
        {
            "src": "/(.*)\\.css",
            "headers": {
                "Content-Type": "text/css"
            },
            "dest": "/$1.css"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}